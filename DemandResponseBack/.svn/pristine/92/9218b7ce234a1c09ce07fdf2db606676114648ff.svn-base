package com.xqxy.dr.modular.upload.entity;


import java.math.BigDecimal;
import java.time.LocalDate;
import java.time.LocalTime;

/**
 * <p>
 * 需求响应方案
 * </p>
 *
 * @author PengChuqing
 * @since 2021-10-08
 */

public class Plan{

    private Long planId;//方案标识

    private Long regulateId;//需求标识

    private String planNo;//方案编号

    private String planName;//方案名称

    private String createOrgName;//编制单位名称

    private String createOrg;//编制单位编号

    private String responseType;//响应类型1削峰，2填谷

    private String timeType;//时间类型 1邀约，2实时

    private String rangeType;//调控范围类别：地区/分区/变电站/线路/台区

    private String regulateRange;//调控范围

    private BigDecimal regulateCap;//调控目标

    private LocalDate regulateDate;//调控日期

    private LocalTime startTime;//响应开始时间

    private LocalTime endTime;//响应结束时间

    private String advanceNoticeTime;//提前通知时间

    private String delRule;//剔除规则

    private String state;//1保存，2生效

    private String checkStatus;//审核状态

    private String particiCondition;//参与条件  [{type:1,value:1,2,3,4},....]

    private String incentiveStandard;//激励标准 格式：1，2，3

    private String validityJudgment;//有效性判定,格式：1，2，3有效性判定,格式：1，2，3

    private String baseLineCal;//基线计算准则 格式：1，2，3

    private Long baselinId;//基线库标识

    public Plan() {
    }

    public Plan(Long planId, Long regulateId, String planNo, String planName, String createOrgName, String createOrg, String responseType, String timeType, String rangeType, String regulateRange, BigDecimal regulateCap, LocalDate regulateDate, LocalTime startTime, LocalTime endTime, String advanceNoticeTime, String delRule, String state, String checkStatus, String particiCondition, String incentiveStandard, String validityJudgment, String baseLineCal, Long baselinId) {
        this.planId = planId;
        this.regulateId = regulateId;
        this.planNo = planNo;
        this.planName = planName;
        this.createOrgName = createOrgName;
        this.createOrg = createOrg;
        this.responseType = responseType;
        this.timeType = timeType;
        this.rangeType = rangeType;
        this.regulateRange = regulateRange;
        this.regulateCap = regulateCap;
        this.regulateDate = regulateDate;
        this.startTime = startTime;
        this.endTime = endTime;
        this.advanceNoticeTime = advanceNoticeTime;
        this.delRule = delRule;
        this.state = state;
        this.checkStatus = checkStatus;
        this.particiCondition = particiCondition;
        this.incentiveStandard = incentiveStandard;
        this.validityJudgment = validityJudgment;
        this.baseLineCal = baseLineCal;
        this.baselinId = baselinId;
    }

    public Long getPlanId() {
        return planId;
    }

    public void setPlanId(Long planId) {
        this.planId = planId;
    }

    public Long getRegulateId() {
        return regulateId;
    }

    public void setRegulateId(Long regulateId) {
        this.regulateId = regulateId;
    }

    public String getPlanNo() {
        return planNo;
    }

    public void setPlanNo(String planNo) {
        this.planNo = planNo;
    }

    public String getPlanName() {
        return planName;
    }

    public void setPlanName(String planName) {
        this.planName = planName;
    }

    public String getCreateOrgName() {
        return createOrgName;
    }

    public void setCreateOrgName(String createOrgName) {
        this.createOrgName = createOrgName;
    }

    public String getCreateOrg() {
        return createOrg;
    }

    public void setCreateOrg(String createOrg) {
        this.createOrg = createOrg;
    }

    public String getResponseType() {
        return responseType;
    }

    public void setResponseType(String responseType) {
        this.responseType = responseType;
    }

    public String getTimeType() {
        return timeType;
    }

    public void setTimeType(String timeType) {
        this.timeType = timeType;
    }

    public String getRangeType() {
        return rangeType;
    }

    public void setRangeType(String rangeType) {
        this.rangeType = rangeType;
    }

    public String getRegulateRange() {
        return regulateRange;
    }

    public void setRegulateRange(String regulateRange) {
        this.regulateRange = regulateRange;
    }

    public BigDecimal getRegulateCap() {
        return regulateCap;
    }

    public void setRegulateCap(BigDecimal regulateCap) {
        this.regulateCap = regulateCap;
    }

    public LocalDate getRegulateDate() {
        return regulateDate;
    }

    public void setRegulateDate(LocalDate regulateDate) {
        this.regulateDate = regulateDate;
    }

    public LocalTime getStartTime() {
        return startTime;
    }

    public void setStartTime(LocalTime startTime) {
        this.startTime = startTime;
    }

    public LocalTime getEndTime() {
        return endTime;
    }

    public void setEndTime(LocalTime endTime) {
        this.endTime = endTime;
    }

    public String getAdvanceNoticeTime() {
        return advanceNoticeTime;
    }

    public void setAdvanceNoticeTime(String advanceNoticeTime) {
        this.advanceNoticeTime = advanceNoticeTime;
    }

    public String getDelRule() {
        return delRule;
    }

    public void setDelRule(String delRule) {
        this.delRule = delRule;
    }

    public String getState() {
        return state;
    }

    public void setState(String state) {
        this.state = state;
    }

    public String getCheckStatus() {
        return checkStatus;
    }

    public void setCheckStatus(String checkStatus) {
        this.checkStatus = checkStatus;
    }

    public String getParticiCondition() {
        return particiCondition;
    }

    public void setParticiCondition(String particiCondition) {
        this.particiCondition = particiCondition;
    }

    public String getIncentiveStandard() {
        return incentiveStandard;
    }

    public void setIncentiveStandard(String incentiveStandard) {
        this.incentiveStandard = incentiveStandard;
    }

    public String getValidityJudgment() {
        return validityJudgment;
    }

    public void setValidityJudgment(String validityJudgment) {
        this.validityJudgment = validityJudgment;
    }

    public String getBaseLineCal() {
        return baseLineCal;
    }

    public void setBaseLineCal(String baseLineCal) {
        this.baseLineCal = baseLineCal;
    }

    public Long getBaselinId() {
        return baselinId;
    }

    public void setBaselinId(Long baselinId) {
        this.baselinId = baselinId;
    }
}
