package com.xqxy.dr.modular.event.enums;

import lombok.Getter;

/**
 * 需求响应方案状态
 */
@Getter
public enum PlanStateEnum {

    SAVE("1", "保存"),

    EFFECT("2", "生效");

    private final String code;

    private final String message;

    PlanStateEnum(String code, String message) {
        this.code = code;
        this.message = message;
    }
}
