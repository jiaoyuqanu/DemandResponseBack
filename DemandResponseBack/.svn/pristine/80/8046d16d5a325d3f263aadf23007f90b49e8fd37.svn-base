package com.xqxy.sys.modular.sms.enums;

import lombok.Getter;

/**
 * 短信发送来源
 */
@Getter
public enum SmsSendSourceEnum {

    SEND_SOURCE_APP(1,"app"),
    SEND_SOURCE_PC(2,"pc"),
    SEND_SOURCE_OTHERS(3,"其他");

    private final Integer code;

    private final String message;

    SmsSendSourceEnum(Integer code, String message) {
        this.code = code;
        this.message = message;
    }
}
