package com.xqxy.dr.modular.upload.entity;

import java.math.BigDecimal;
import java.time.LocalDate;


/**
 * <p>
 * 需求响应事件
 * </p>
 *
 * @author PengChuqing
 * @since 2021-10-08
 */

public class Event{

    private Long eventId;//事件标识

    private Long regulateId;//方案标识

    private Long projectId;//项目标识

    private Long eventNo;//事件编号

    private String eventName;//事件名称

    private String responseType;//响应类型

    private String timeType;//时间类型

    private String rangeType;//调控范围类别：地区/分区/变电站/线路/台区

    private String regulateRange;//调控范围

    private BigDecimal regulateCap;//调控目标

    private LocalDate regulateDate;//调控日期

    private String startTime;//响应开始时间

    private String endTime;//响应结束时间

    private String advanceNoticeTime;//提前通知时间

    private String eventStatus;//事件状态 1:保存，2待执行，3执行中，4结束，5废弃

    private String checkStatus;//审核状态 1:未提交，2：审核中，3：审核通过，4：审核不通过

    private String particiCondition;//参与条件  [{type:1,value:1,2,3,4},....]

    private String incentiveStandard;//激励标准 格式：1，2，3

    private String validityJudgment;//有效性判定,格式：1，2，3

    private String baseLineCal;//基线计算准则 格式：1，2，3

    private Long baselinId;//基线库标识

    private ConsSubsidy consSubsidy;

    private BigDecimal contractPrice;//签约价格

    private BigDecimal subsidyAmount;//激励金额

    private BigDecimal actualCap;//实际响应负荷

    private ConsInvitation consInvitation;

    private BigDecimal invitationCap;//邀约响应量

    private Consevaluation consevaluation;

    private Long evaluationId;//评估标识

    public Event() {
    }

    public Event(Long eventId, Long regulateId, Long projectId, Long eventNo, String eventName, String responseType, String timeType, String rangeType, String regulateRange, BigDecimal regulateCap, LocalDate regulateDate, String startTime, String endTime, String advanceNoticeTime, String eventStatus, String checkStatus, String particiCondition, String incentiveStandard, String validityJudgment, String baseLineCal, Long baselinId, ConsSubsidy consSubsidy, BigDecimal contractPrice, BigDecimal subsidyAmount, BigDecimal actualCap, ConsInvitation consInvitation, BigDecimal invitationCap, Consevaluation consevaluation, Long evaluationId) {
        this.eventId = eventId;
        this.regulateId = regulateId;
        this.projectId = projectId;
        this.eventNo = eventNo;
        this.eventName = eventName;
        this.responseType = responseType;
        this.timeType = timeType;
        this.rangeType = rangeType;
        this.regulateRange = regulateRange;
        this.regulateCap = regulateCap;
        this.regulateDate = regulateDate;
        this.startTime = startTime;
        this.endTime = endTime;
        this.advanceNoticeTime = advanceNoticeTime;
        this.eventStatus = eventStatus;
        this.checkStatus = checkStatus;
        this.particiCondition = particiCondition;
        this.incentiveStandard = incentiveStandard;
        this.validityJudgment = validityJudgment;
        this.baseLineCal = baseLineCal;
        this.baselinId = baselinId;
        this.consSubsidy = consSubsidy;
        this.contractPrice = contractPrice;
        this.subsidyAmount = subsidyAmount;
        this.actualCap = actualCap;
        this.consInvitation = consInvitation;
        this.invitationCap = invitationCap;
        this.consevaluation = consevaluation;
        this.evaluationId = evaluationId;
    }

    public Long getEventId() {
        return eventId;
    }

    public void setEventId(Long eventId) {
        this.eventId = eventId;
    }

    public Long getRegulateId() {
        return regulateId;
    }

    public void setRegulateId(Long regulateId) {
        this.regulateId = regulateId;
    }

    public Long getProjectId() {
        return projectId;
    }

    public void setProjectId(Long projectId) {
        this.projectId = projectId;
    }

    public Long getEventNo() {
        return eventNo;
    }

    public void setEventNo(Long eventNo) {
        this.eventNo = eventNo;
    }

    public String getEventName() {
        return eventName;
    }

    public void setEventName(String eventName) {
        this.eventName = eventName;
    }

    public String getResponseType() {
        return responseType;
    }

    public void setResponseType(String responseType) {
        this.responseType = responseType;
    }

    public String getTimeType() {
        return timeType;
    }

    public void setTimeType(String timeType) {
        this.timeType = timeType;
    }

    public String getRangeType() {
        return rangeType;
    }

    public void setRangeType(String rangeType) {
        this.rangeType = rangeType;
    }

    public String getRegulateRange() {
        return regulateRange;
    }

    public void setRegulateRange(String regulateRange) {
        this.regulateRange = regulateRange;
    }

    public BigDecimal getRegulateCap() {
        return regulateCap;
    }

    public void setRegulateCap(BigDecimal regulateCap) {
        this.regulateCap = regulateCap;
    }

    public LocalDate getRegulateDate() {
        return regulateDate;
    }

    public void setRegulateDate(LocalDate regulateDate) {
        this.regulateDate = regulateDate;
    }

    public String getStartTime() {
        return startTime;
    }

    public void setStartTime(String startTime) {
        this.startTime = startTime;
    }

    public String getEndTime() {
        return endTime;
    }

    public void setEndTime(String endTime) {
        this.endTime = endTime;
    }

    public String getAdvanceNoticeTime() {
        return advanceNoticeTime;
    }

    public void setAdvanceNoticeTime(String advanceNoticeTime) {
        this.advanceNoticeTime = advanceNoticeTime;
    }

    public String getEventStatus() {
        return eventStatus;
    }

    public void setEventStatus(String eventStatus) {
        this.eventStatus = eventStatus;
    }

    public String getCheckStatus() {
        return checkStatus;
    }

    public void setCheckStatus(String checkStatus) {
        this.checkStatus = checkStatus;
    }

    public String getParticiCondition() {
        return particiCondition;
    }

    public void setParticiCondition(String particiCondition) {
        this.particiCondition = particiCondition;
    }

    public String getIncentiveStandard() {
        return incentiveStandard;
    }

    public void setIncentiveStandard(String incentiveStandard) {
        this.incentiveStandard = incentiveStandard;
    }

    public String getValidityJudgment() {
        return validityJudgment;
    }

    public void setValidityJudgment(String validityJudgment) {
        this.validityJudgment = validityJudgment;
    }

    public String getBaseLineCal() {
        return baseLineCal;
    }

    public void setBaseLineCal(String baseLineCal) {
        this.baseLineCal = baseLineCal;
    }

    public Long getBaselinId() {
        return baselinId;
    }

    public void setBaselinId(Long baselinId) {
        this.baselinId = baselinId;
    }

    public ConsSubsidy getConsSubsidy() {
        return consSubsidy;
    }

    public void setConsSubsidy(ConsSubsidy consSubsidy) {
        this.consSubsidy = consSubsidy;
    }

    public BigDecimal getContractPrice() {
        return contractPrice;
    }

    public void setContractPrice(BigDecimal contractPrice) {
        this.contractPrice = contractPrice;
    }

    public BigDecimal getSubsidyAmount() {
        return subsidyAmount;
    }

    public void setSubsidyAmount(BigDecimal subsidyAmount) {
        this.subsidyAmount = subsidyAmount;
    }

    public BigDecimal getActualCap() {
        return actualCap;
    }

    public void setActualCap(BigDecimal actualCap) {
        this.actualCap = actualCap;
    }

    public ConsInvitation getConsInvitation() {
        return consInvitation;
    }

    public void setConsInvitation(ConsInvitation consInvitation) {
        this.consInvitation = consInvitation;
    }

    public BigDecimal getInvitationCap() {
        return invitationCap;
    }

    public void setInvitationCap(BigDecimal invitationCap) {
        this.invitationCap = invitationCap;
    }

    public Consevaluation getConsevaluation() {
        return consevaluation;
    }

    public void setConsevaluation(Consevaluation consevaluation) {
        this.consevaluation = consevaluation;
    }

    public Long getEvaluationId() {
        return evaluationId;
    }

    public void setEvaluationId(Long evaluationId) {
        this.evaluationId = evaluationId;
    }
}
